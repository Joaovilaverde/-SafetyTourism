@model SafetyTourism.Models.Pais

@{
    ViewData["Title"] = "Detalhes do país";
}


<div>
    <h4>Detalhes do país</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Nome
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
        <dt class="col-sm-2">
            Id da zona
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ZonaId)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Editar</a> |
    <a asp-action="Index">Voltar à lista</a>
</div>
